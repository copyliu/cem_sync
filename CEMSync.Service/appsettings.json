{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "System.Net.Http.HttpClient": "Warning"
        },
        "NLog": {
            "IncludeScopes": true
        }
    },

    "NLog": {
        "throwConfigExceptions": true,
        "targets": {

            "logconsole": {
                "type": "ColoredConsole"
            },
            "BlackHole": {
                "type": "Null"
            }
        },
        "rules": [
            {
                "logger": "Microsoft.EntityFrameworkCore.Database.Command",
                "maxLevel": "Info",
                "final": true

            },

            {
                "logger": "Microsoft.EntityFrameworkCore.*",
                "maxLevel": "Info",

                "final": true

            },
            {
                "logger": "Microsoft.Extensions.Http.DefaultHttpClientFactory",
                "writeTo": "BlackHole",
                "final": true
            },
            {
                "logger": "System.Net.Http.HttpClient.*",
                "writeTo": "BlackHole",
                "final": true
            },
            {
                "logger": "*",
                "minLevel": "Debug",
                "writeTo": "logconsole"
            }


        ]
    },

    "ConnectionStrings": {
        "EVEMapsDB": "Host=localhost;Database=evemaps;Username=postgres;Password=;",
        "CNKillboardDB": "Host=localhost;Port=5432;Database=ceve_kb;Username=postgres;Password=;",
        "TQKillboardDB": "Host=localhost;Port=5432;Database=ceve_tqkb;Username=postgres;Password=;",
        "MarketDB": "Host=localhost;Port=5432;Database=cevemarket;Username=postgres;Password=;",
        "MarketDB_TQ": "Host=localhost;Port=5432;Database=cevemarket;Username=postgres;Password=;Search Path=tq"
    },
    "EsiConfig": {
        "EsiUrl": "https://esi.evepc.163.com/",
        "DataSource": "Serenity",
        "ClientId": "**********",
        "SecretKey": "**********",
        "CallbackUrl": "**********",
        "UserAgent": "KB.CEVE-MARKET.ORG CORE VERSION @COPYLIU"
    },
    "tq": false
}